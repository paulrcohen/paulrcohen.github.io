us act of violence.\par \pard \widctlpar \par \pard \widctlpar {{\pict\macpict\picw377\pich103\picwgoal7540\pichgoal2060 0957ffffffff006601781101a00082a100b600040026008001000affffffff006601780700000000220045012e0000a000a0a100a400020b260aff00ff00ff00ff00740022000900e50045012e001800e50045012e002f012d000900ec000900e5001800e501000a0000000000000000070001000122001800e5492d23000023ffea23000023bfda23000023f90023000023000f23000084000a0000000000000000a000a301000affffffff0066017884000a000000000000000023492d23ffea23bfda23f90023000fa000a1a100b60004000100010700000000230000a100b600040026008009882288228822882231001e012d00530145070001000109ffffffffffffffff38a100b60004000100010700000000230000a100b600040026008009882288228822882231003c006b00530074070001000109ffffffffffffffff38a100b60004000100010700000000230000a0008ca100b600040026008007000100013000090023005300b4a100b60004000100010700000000230000a100b6000400260080a100d80004ffff8000a100d60004ffff8000a000d7a100b4000500000203040700010001080017220009006b0049a000b5a100d60004ffff8000a000d7a000d9a100d60004ffff8000a100d80004ffff8000a000bea000d9a0008c080008220009006b0002220010006b0002220017006b000222001e006b0002220025006b000222002c006b0002220033006b000222003a006b0002220041006b0002220048006b000222004f006b0002a0008da100d80004ffff8000a000bfa000d9a000d7a100b60004000100010700000000230000a100b6000400260080a0008c71001e002e00630033006a0033006a0032006300300064002e00650033006a070001000122000a00234126a0008da100b60004000100010700000000230000a100b60004002600800700010001220051006d08cea100b60004000100010700000000230000a10096000c010000000200000000000000a1009a000800020000000d000001000a00540056006400722c000c00150948656c7665746963610300150d00082e0004000001002b575a09706f696e74206f660d2a0807636f6e74616374a00097a10096000c010000000200000000000000a1009a000800020000000e000001000a002200000032001e28002800010a72656c6174697665200d2a080876656c6f63697479a00097a10096000c010000000200000000000000a1009a0008fffe00000001000001000a004a00150052001b2b15200130a00097a10096000c0